# Copy this to .env and fill in your actual values
# DO NOT COMMIT .env TO GIT!

# Alchemy API Key (get from https://www.alchemy.com/)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Your wallet's private key (DO NOT SHARE THIS!)
# You can export this from MetaMask: Account Menu > Account Details > Export Private Key
PRIVATE_KEY=your_private_key_here

# Etherscan API Key (get from https://etherscan.io/apis)
ETHERSCAN_API_KEY=your_etherscan_api_key_here
